   "source": [
    "def manhattan(setA,setB):\n",
    "    distance = 0\n",
    "    for itemA,itemB in zip(setA,setB):\n",
    "        distance += sum([abs(itemA-itemB)])\n",
    "    return distance\n",
    "print(\"Manhattan Distance: \", manhattan([2,5,7,8,9],[7,5,8,6,3]))"
   ]
