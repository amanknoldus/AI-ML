    "def chebyshev(setA,setB):\n",
    "    distance=[]\n",
    "    for indexA,indexB in zip(setA,setB):\n",
    "        distance.append(abs(indexA-indexB))\n",
    "        print(distance)\n",
    "    return max(distance)\n",
    "print(\"Chebyshev Distance:\",chebyshev([1,3,4,1],[3,2,1,1]))"
   ]
