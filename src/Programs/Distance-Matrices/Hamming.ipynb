   "source": [
    "def hamming(setA,setB):\n",
    "    distance = 0\n",
    "    for indexofA,indexofB in zip(setA,setB):\n",
    "        if indexofA != indexofB:\n",
    "            distance += 1\n",
    "    return distance\n",
    "print(\"Hamming Distance:\",hamming(\"Aman Srivastava\",\"Aman Yadev\"))"
   ]
